    "i18next-browser-languagedetector": "7.2.1",
    "i18next-http-backend": "2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.1.0",
    "react-router-dom": "6.22.3",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
